    "buildProduction": "SET NODE_ENV=production && webpack -d --color",
    "lint": "tslint ./angular2app/**/*.ts -t verbose",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings"
  },
  "keywords": [],
  "author": "Fabian Gosebrink",
